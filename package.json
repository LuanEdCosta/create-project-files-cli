{
  "name": "create-project-files-cli",
  "description": "Create your project files faster using a CLI",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Luan Eduardo da Costa",
    "url": "https://github.com/LuanEdCosta"
  },
  "main": "lib/index.js",
  "bin": {
    "cpf": "./lib/index.js"
  },
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc",
    "local-windows": "yarn build && yarn global add file:%INIT_CWD%",
    "local-unix": "yarn build && yarn global add file:$INIT_CWD",
    "publish": "yarn build && npm publish",
    "files-publish": "npm pack --dry-run",
    "dev": "node -r ts-node/register src/index.ts"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "ora": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.3.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "files": [
    "lib/*",
    "package.json",
    "LICENSE",
    "README.md"
  ]
}
